pipeline{
	agent any
	tools{
		maven "maven3.9.9"
	}
	stages{
		stage("build"){
			steps{
				echo "building the app"
				sh "mvn install"
			}
		}
		stage("Sonarqube analysis"){
			steps{
				withSonarQubeEnv(credentialsId: 'sonarID2') {
    				sh 'mysonar'
				}
			}
		}
		stage("deploy"){
			steps{
				echo "Deploying the apps"
			}
		}
	}
}